@model int
<div class="col-xs-12" data-bind="if: !!punctuality()"><h1 data-bind="text: punctuality().name"></h1></div>
<!-- ko foreach: houses -->
<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
    <div class="panel panel-default" data-bind="css: !$root.all() && arrived || arrived === scouts.length ? 'panel-success':'panel-danger' ">
        <div class="panel-heading" data-bind="text: name"></div>
        <ul class="list-group" data-bind="visible: $root.all(), foreach: scouts">
            <li class="list-group-item"><span data-bind="text: name"></span> <span class="glyphicon glyphicon-ok" data-bind="visible: arrived"></span></li>
        </ul>
    </div>
</div>
<!-- /ko -->
@section Body {data-bind="event: { keydown: function(d,e) { rfid.keydown(e); return true; } }"}

@section Scripts
{
    <script type="text/javascript">
        $(function() {
            ko.applyBindings(new Punctuality.Station.App(@Model));
        });
    </script>
}